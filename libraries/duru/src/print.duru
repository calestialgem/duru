public void print(byte* string) {
  int32 result = print_string_to_stream(string, get_error_stream());
  if (result < 0) {
    exit(result);
  }
}

extern "FILE"
struct stream;

extern "__acrt_iob_func"
stream* get_stream(uint32 index);

extern "fputs"
int32 print_string_to_stream(byte* string, stream* destination);

stream* get_error_stream() {
  return get_stream(2);
}
